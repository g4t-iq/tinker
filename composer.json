{
    "name": "harmony-framework/tinker",
    "description": "Powerful REPL for the harmony framework.",
    "keywords": ["tinker", "repl", "psysh", "harmony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@harmony.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "G4T/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "G4T/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "G4T/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "psy/psysh": "^0.10.4|^0.11.1",
        "symfony/var-dumper": "^4.3.4|^5.0|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.3|^1.4.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "suggest": {
        "G4T/database": "The G4T Database package (^6.0|^7.0|^8.0|^9.0|^10.0)."
    },
    "autoload": {
        "psr-4": {
            "Harmony\\Tinker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Harmony\\Tinker\\Tests\\": "tests/",
            "App\\": "tests/fixtures/app",
            "One\\Two\\": "tests/fixtures/vendor/one/two"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "harmony": {
            "providers": [
                "harmony\\Tinker\\TinkerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
